<table class="table table-striped">
    <thead>
    <tr>
        <th></th>
        <th>questions</th>
        <th>Level</th>
    </tr>
    </thead>
    <tbody>
    {% for question in questions %}
        <tr>
            <td><input type="checkbox" value="{{ question.id }}"></td>
            <td>{{ question.id }}</td>
            <td>{{ question.name }}</td>
            <td>{{ question.level }}</td>
        </tr>
    {% else %}
        <tr>
            <td colspan="5">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
